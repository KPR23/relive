{
  "name": "relive",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "turbo": "^2.8.10",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "overrides": {
      "@trpc/server": "10.45.4",
      "@trpc/client": "10.45.4",
      "@trpc/react-query": "10.45.4",
      "typescript": "5.9.3",
      "zod": "^4.3.5",
      "rxjs": "^7.8.2"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "nestjs-trpc>@nestjs/common": "11",
        "nestjs-trpc>@nestjs/core": "11"
      }
    },
    "patchedDependencies": {
      "nestjs-trpc@1.6.1": "patches/nestjs-trpc@1.6.1.patch"
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "msw",
      "sharp",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.30.0",
  "engines": {
    "node": ">=18"
  }
}
