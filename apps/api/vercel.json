{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build",
  "functions": {
    "dist/src/main.js": {
      "runtime": "@vercel/node@3",
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/src/main.js"
    }
  ]
}
